{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "module": "ES2020",
    "moduleResolution": "Node",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "target": "ES2019",
    "declaration": false,
    "noEmit": true
  },
  "include": ["./src/**/*.ts"]
}
